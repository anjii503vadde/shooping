{
    "extends": "airbnb-base",
    "env": 
    {
        "browser": true,
        "jquery": true
    },
    "globals": 
    {
        "document": false
    },
    "rules": 
    {
        "import/prefer-default-export": "off",
        "linebreak-style": 0,
        "func-names" :0,
        "no-unused-vars" : 0,
        "allowEmptyReject": true,
        "no-param-reassign": 0,
        "no-use-before-define": 0,
        "class-methods-use-this": 0,
        "prefer-destructuring": 0
    }
}